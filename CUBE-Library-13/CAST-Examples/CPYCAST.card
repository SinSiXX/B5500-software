?COMPILE COPY/CAST ALGOL GO
?DATA CARD
$CARD LIST SINGLE
BEGIN
ARRAY REC[0:55];
FILE IN CASTC (2, 56);
FILE OUT CASTA DISK SERIAL [20:960] "CASTA" "LIBRARY"
        (2, 56, SAVE 30);
LABEL EOF;
WHILE TRUE DO
  BEGIN
  READ(CASTC, 56, REC[*]) [EOF];
  WRITE(CASTA, 56, REC[*]);
  END;
EOF:
LOCK(CASTA);
END.
?END
