PROCEDURE FOURIER(A,B,N,X,Y);VALUE N,X;INTEGER N,X;REAL Y;REAL ARRAY A[000000400
],B[1];BEGIN REAL ARRAY C[0:N],D[0:N];INTEGER I;C[0]~1;D[0]~0;C[1]~COS(X00000500
);D[1]~SIN(X);FOR I~2 STEP 1 UNTIL N DO BEGIN C[I]~2|C[I-1]|C[1]-C[I-2];00000600
D[I]~2|D[I-1]|C[1]-D[I-2]END;Y~A[0];FOR I~1 STEP 1 UNTIL N DO Y~A[I]|C[I00000700
]+B[I]|D[I]+Y ;END;                                                     00000800
